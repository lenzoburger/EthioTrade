{
  "name": "eth-coffee-spa",
  "version": "0.0.0",
  "scripts": {
    "ng": "cd EthCoffee-SPA && ng",
    "dotnet": "cd EthCoffee.api && dotnet",
    "start": "concurrently \"npm run start-api\" \"npm run start-spa\"",
    "start-only": "concurrently \"npm run ng serve\" \"npm run dotnet run\"",
    "start-api": "npm run dotnet watch run",
    "start-spa": "cd EthCoffee-SPA && ng serve --open",
    "build-spa": "npm run ng build",
    "build-spa-prod": "cd EthCoffee-SPA && ng build --prod",
    "build-api": "npm run dotnet restore && npm run dotnet build",
    "rebuild-prod": "cd EthCoffee-SPA && ng build --prod && cd .. && npm run dotnet watch run",
    "setup": "npm install && npm run build-api",
    "test": "npm run ng test",
    "lint": "npm run ng lint",
    "e2e": "npm run ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@auth0/angular-jwt": "^3.0.1",
    "@ngtools/webpack": "^8.3.20",
    "alertifyjs": "^1.12.0",
    "bootstrap": "^4.4.1",
    "bootswatch": "^4.4.0",
    "core-js": "^3.4.5",
    "font-awesome": "^4.7.0",
    "moment": "^2.24.0",
    "ng2-file-upload": "^1.4.0",
    "ngx-bootstrap": "^5.2.0",
    "ngx-gallery": "^5.10.0",
    "rxjs": "^6.5.3",
    "time-ago-pipe": "^1.3.2",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.20",
    "@angular/cli": "^8.3.20",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@types/jasmine": "^3.5.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.12.14",
    "codelyzer": "^5.0.1",
    "concurrently": "^5.0.0",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "^5.4.2",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typescript": "^3.5.3"
  }
}
